.title .after,.title .before{
  width:42%;
  position:absolute;
  top:50%;
  left:15rpx;
  transform:translateY(-50%);
  height:1px;
  background-color:#EEE;
}
.title .before{
  left:auto;
  right:15rpx;
}
.title{
  width:100%;
  height:70rpx;
  line-height:70rpx;
  text-align: center;
  color:#666;
  font-size:36rpx;
  position:relative;
}