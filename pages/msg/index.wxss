.search{
  width:440rpx;
  border:solid 1px #EEE;
  border-radius:100rpx;
  height:70rpx;
  line-height:70rpx;
  margin:0 auto;
  padding:0 80rpx 0 20rpx;
  box-sizing:border-box;
  margin-top:40rpx;
}
.search_box{
  width:440rpx;
  margin:0 auto;
  position:relative;
}
icon{
  position:absolute;
  top:50%;
  transform: translateY(-50%);
  right:15rpx;
  z-index:99999;
}
.title{
  line-height:70rpx;
}